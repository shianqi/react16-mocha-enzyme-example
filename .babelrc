            "last 2 versions",
            "ie >= 9"
          ],
          "node": "current"
        },
        "debug": false,
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-3"
  ],
  "plugins": [
    "react-hot-loader/babel"
  ],
  "sourceMaps": "inline"
}
